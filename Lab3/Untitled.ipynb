    "import nltk\n",
    "\n",
    "dwlr = nltk.downloader.Downloader()\n",
    "\n",
    "for pkg in dwlr.corpora():\n",
    "    dwlr._status_cache[pkg.id] = 'installed'\n",
    "\n",
    "dwlr.download('popular')"
